{
    "rest-client.enableTelemetry": false,
    "rest-client.environmentVariables": {
        "local": {
          "host": "https://localhost:7178",
          "apimkey": ""
        },
        "prod-apim": {
          "host": "https://api.cezzis.com/prd/cocktails",
          "apimkey": ""
        }
    },
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.enableEditorConfigSupport": true,
    "dotnet.defaultSolution": "Cocktails.Api.sln",
    "files.exclude": {
        "**/bin": true,
        "**/obj": true
    },
    "search.exclude": {
        "**/bin": true,
        "**/obj": true
    },
    "yaml.schemas": {
        "https://raw.githubusercontent.com/GitTools/GitVersion/main/schemas/6.3/GitVersion.json": "GitVersion.yml"
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.cs": "${capture}.http",
        "*.json": "${capture}.*.json,${capture}.*.json.user",
        "*.resx": "${capture}.Designer.cs",
        "*.yaml": "${capture}.override.yaml"
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true
    }
}